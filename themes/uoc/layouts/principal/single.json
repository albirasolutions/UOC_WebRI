[{{ range $i, $s := .Site.AllPages }}{{- if and (eq $s.Type "solucio_tec") (ne $s.File.UniqueID "")}}{
    "type" : "add",
    "id" : {{ $s.File.UniqueID | jsonify }},
    "fields" : {
        {{- if and (isset $s.Params "language") (ne $s.Params.language nil) }}
            "idioma": {{$s.Params.language | jsonify}}{{- end }}
    }
}{{ if ne 47 $i}},{{end}}{{end}}{{end}}]
